{
    "plugins": [
        "category"
    ],
    "opts": {
        "encoding": "utf8",
        "destination": "docs/",
        "recurse": true,
        "readme": "README.md"
    },
    "templates": {
        "default": {
            "useLongnameInNav": false,
            "outputSourceFiles": true
        },
        "staticFiles": {
            "include": [
                "./static"
            ]
        },
        "bepro": {
            "name": "bepro-js documentation",
            "logo": "images/logo.png",
            "style": "style.css",
            "analyticsTracking": "",
            "navLinks": [
                {
                    "label": "Github",
                    "href": "https://github.com/bepronetwork/bepro-js"
                }
            ]
        }
    }
}
