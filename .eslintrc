{
  "parser": "babel-eslint",
  "extends": [
    "airbnb-base"
  ],
  "rules": {
      "import/no-unresolved": [
        2, 
        { "caseSensitive": false }
    ],
      "no-prototype-builtins": 0,
      "no-return-await": 0,
      "camelcase": 0,
      "no-underscore-dangle": 0,
      "class-methods-use-this": 1,
      "no-unused-vars": 1,
      "max-len": 1,
      "no-undef": 1,
      "eqeqeq": 1,
      "no-mixed-operators" : 1
  }
}